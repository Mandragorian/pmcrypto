  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProtonMail/pmcrypto.git"
  },
  "author": "ProtonMail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ProtonMail/pmcrypto/issues"
  },
  "ava": {
