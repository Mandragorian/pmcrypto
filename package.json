      "test/**/*.js",
      "!test/**/*.data.js",
      "!test/helper.js"
    ]
  },
  "homepage": "https://github.com/ProtonMail/pmcrypto#readme",
  "dependencies": {
    "@types/openpgp": "^4.4.12",
    "esm": "^3.0.84",
